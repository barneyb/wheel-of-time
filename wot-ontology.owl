Prefix(:=<http://wot.barneyb.com/wot/>)
Prefix(dc:=<http://purl.org/dc/terms/>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)


Ontology(<http://wot.barneyb.com/wot/>

Declaration(Class(:Angreal))
Declaration(Class(:Book))
Declaration(Class(:Chapter))
Declaration(Class(:Clothing))
Declaration(Class(:Event))
Declaration(Class(:Horse))
Declaration(Class(:Human))
Declaration(Class(:Item))
Declaration(Class(:LivingThing))
Declaration(Class(:ObjectOfPower))
Declaration(Class(:Ogier))
Declaration(Class(:Saangreal))
Declaration(Class(:StoryPart))
Declaration(Class(:Terangreal))
Declaration(Class(:Vehicle))
Declaration(Class(:Weapon))
Declaration(Class(:Wolf))
Declaration(ObjectProperty(dc:hasPart))
Declaration(ObjectProperty(dc:isPartOf))
Declaration(DataProperty(:chapterNumber))
Declaration(NamedIndividual(:TEotW_AnEmptyRoad))
Declaration(NamedIndividual(:TheEyeOfTheWorld))
Declaration(AnnotationProperty(dc:date))
Declaration(AnnotationProperty(dc:identifier))
Declaration(AnnotationProperty(dc:title))
Declaration(Datatype(xsd:date))
############################
#   Annotation Properties
############################

# Annotation Property: dc:title (dc:title)

SubAnnotationPropertyOf(dc:title rdfs:label)


############################
#   Object Properties
############################

# Object Property: dc:hasPart (hasPart)

AnnotationAssertion(rdfs:label dc:hasPart "hasPart")
InverseObjectProperties(dc:hasPart dc:isPartOf)
TransitiveObjectProperty(dc:hasPart)

# Object Property: dc:isPartOf (isPartOf)

AnnotationAssertion(rdfs:label dc:isPartOf "isPartOf")
TransitiveObjectProperty(dc:isPartOf)


############################
#   Data Properties
############################

# Data Property: :chapterNumber (:chapterNumber)

FunctionalDataProperty(:chapterNumber)
DataPropertyRange(:chapterNumber xsd:integer)



############################
#   Classes
############################

# Class: :Angreal (:Angreal)

SubClassOf(:Angreal :ObjectOfPower)

# Class: :Book (:Book)

SubClassOf(:Book :StoryPart)
DisjointClasses(:Book :Chapter)

# Class: :Chapter (:Chapter)

SubClassOf(:Chapter :StoryPart)

# Class: :Clothing (:Clothing)

SubClassOf(:Clothing :Item)

# Class: :Event (:Event)


# Class: :Horse (:Horse)

SubClassOf(:Horse :LivingThing)

# Class: :Human (:Human)

SubClassOf(:Human :LivingThing)

# Class: :Item (:Item)


# Class: :LivingThing (:LivingThing)


# Class: :ObjectOfPower (Object of Power)

AnnotationAssertion(rdfs:label :ObjectOfPower "Object of Power")
SubClassOf(:ObjectOfPower :Item)

# Class: :Ogier (:Ogier)

SubClassOf(:Ogier :LivingThing)

# Class: :Saangreal (Sa'angreal)

AnnotationAssertion(rdfs:label :Saangreal "Sa'angreal")
SubClassOf(:Saangreal :ObjectOfPower)

# Class: :StoryPart (:StoryPart)


# Class: :Terangreal (Ter'angreal)

AnnotationAssertion(rdfs:label :Terangreal "Ter'angreal")
SubClassOf(:Terangreal :ObjectOfPower)

# Class: :Vehicle (:Vehicle)

SubClassOf(:Vehicle :Item)

# Class: :Weapon (:Weapon)

SubClassOf(:Weapon :Item)

# Class: :Wolf (:Wolf)

SubClassOf(:Wolf :LivingThing)


############################
#   Named Individuals
############################

# Individual: :TEotW_AnEmptyRoad (An Empty Road)

AnnotationAssertion(rdfs:label :TEotW_AnEmptyRoad "An Empty Road")
ClassAssertion(:Chapter :TEotW_AnEmptyRoad)
ObjectPropertyAssertion(dc:isPartOf :TEotW_AnEmptyRoad :TheEyeOfTheWorld)
DataPropertyAssertion(:chapterNumber :TEotW_AnEmptyRoad "1"^^xsd:integer)

# Individual: :TheEyeOfTheWorld (The Eye of the World)

AnnotationAssertion(dc:date :TheEyeOfTheWorld "1990-01-15"^^xsd:date)
AnnotationAssertion(dc:identifier :TheEyeOfTheWorld "TEofW")
AnnotationAssertion(dc:title :TheEyeOfTheWorld "The Eye of the World")
AnnotationAssertion(rdfs:label :TheEyeOfTheWorld "The Eye of the World")
ClassAssertion(:Book :TheEyeOfTheWorld)


DisjointClasses(:Angreal :Saangreal :Terangreal)
DisjointClasses(:Event :Item :LivingThing :StoryPart)
DisjointClasses(:Horse :Human :Ogier :Wolf)
)