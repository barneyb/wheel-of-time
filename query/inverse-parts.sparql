prefix dcterms: <http://purl.org/dc/terms/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix : <http://wot.barneyb.com/wot/>
SELECT ?whole ?subject ?part
WHERE {
      {
          { ?subject dcterms:isPartOf ?whole }
          union
          { ?subject :hasPart ?part }
      }
      optional { ?subject rdfs:label ?title }
      filter ( !bound(?title) || strstarts(?title, "K") )
      }
order by ?title ?whole ?part
